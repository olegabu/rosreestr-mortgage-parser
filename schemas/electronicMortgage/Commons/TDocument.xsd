        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SDocName">
        <xs:annotation>
            <xs:documentation>Наименование документа</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="1024"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SDocTypeCode">
        <xs:annotation>
            <xs:documentation>Мнемоника типа документа</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:token"/>
