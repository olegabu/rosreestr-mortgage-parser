        <xs:annotation>
            <xs:documentation>Мнемоника типа документа согласно справочника "Документы" ЕС УНСИ</xs:documentation>
        </xs:annotation>
        <xs:restriction base="dDocument:DDocument">
            <xs:pattern value="558401\d{6}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="DBaseRepresentativeDocument">
        <xs:restriction base="dDocument:DDocument">
            <xs:pattern value="558301\d{6}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="DRepresentativeDocument">
        <xs:annotation>
            <xs:documentation>Документы, подтверждающие полномочия представителя</xs:documentation>
        </xs:annotation>
        <xs:restriction base="tns:DBaseRepresentativeDocument">
            <xs:pattern value="5583010[2-9]\d{4}|5583011\d{5}"/>
        </xs:restriction>
    </xs:simpleType>
