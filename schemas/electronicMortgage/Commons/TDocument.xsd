        <xs:annotation>
            <xs:documentation>Код причины постановки на учет</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="\d{4}[\w\d]{2}\d{3}"/>
        </xs:restriction>
    </xs:simpleType>
