        <xs:annotation>
            <xs:documentation>Страховой номер индивидуального лицевого счёта (СНИЛС)</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="\d{3}\-?\d{3}\-?\d{3}\s?\d{2}"/>
        </xs:restriction>
    </xs:simpleType>

