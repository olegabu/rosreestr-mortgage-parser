        <xs:annotation>
            <xs:documentation>Идентификационный номер налогоплательщика (ИНН) организации</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:length value="10"/>
            <xs:pattern value="\d+"/>
        </xs:restriction>
    </xs:simpleType>

