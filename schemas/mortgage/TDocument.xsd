        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:length value="9"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SSettlementAccount">
        <xs:annotation>
            <xs:documentation>Расчётный счёт</xs:documentation>
