<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified"
    xmlns:tns="http://rosreestr.ru/services/v0.1/commonsMortgage/Documents"
    xmlns:commons="http://rosreestr.ru/services/v0.1/commonsMortgage/Commons"
    xmlns:siml="http://rosreestr.ru/services/v0.1/commonsMortgage/simple-types"
    xmlns:dDocument="http://rosreestr.ru/services/v0.1/commonsMortgage/directories/document"
    xmlns:address="http://rosreestr.ru/services/v0.1/commonsMortgage/Address"
    xmlns:subj="http://rosreestr.ru/services/v0.1/commonsMortgage/Subjects"
    xmlns:subj1="http://rosreestr.ru/services/v0.1/commonsMortgage/Subjects"
    targetNamespace="http://rosreestr.ru/services/v0.1/commonsMortgage/Documents">

    <xs:import namespace="http://rosreestr.ru/services/v0.1/commonsMortgage/Subjects"
        schemaLocation="TSubject.xsd"/>
    <xs:import namespace="http://rosreestr.ru/services/v0.1/commonsMortgage/Commons"
        schemaLocation="Commons.xsd"/>
    <xs:import namespace="http://rosreestr.ru/services/v0.1/commonsMortgage/simple-types"
        schemaLocation="CommonSimpleType.xsd"/>
    <xs:import namespace="http://rosreestr.ru/services/v0.1/commonsMortgage/directories/document"
        schemaLocation="Dictionary/DDocument.xsd"/>
    <xs:import namespace="http://rosreestr.ru/services/v0.1/commonsMortgage/Address"
        schemaLocation="TAddress.xsd"/>

    <xs:complexType name="TDocumentRefer">
        <xs:annotation>
            <xs:documentation>Ссылка на документ по его идентификатору</xs:documentation>
        </xs:annotation>
        <xs:attribute name="documentID" use="required" type="xs:token">
            <xs:annotation>
                <xs:documentation>Идентификатор документа</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="TBasicSomeDocument">
        <xs:annotation>
            <xs:documentation>Базовый класс для групп документов в header у МИВ и ЕГРП</xs:documentation>
        </xs:annotation>
        <xs:choice>
            <xs:element name="document" type="tns:TDocument" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Документ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="otherDocument" type="tns:TOtherDocumentRestr">
                <xs:annotation>
                    <xs:documentation>Прочий документ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="paymentDocument" type="tns:TPaymentDocumentRestr" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Платежный документ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="idDocument" type="tns:TIdDocumentRestr" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Документ, удостоверяющий личность</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="powerOfAttorney" type="tns:TPowerOfAttorneyRestr" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Доверенность</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="mapPlanDocument" type="tns:TMapPlanDocumentRestr" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Тех. план/межевой план/акт обследования/карта-план</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="interdepDocument" type="tns:TInterdepDocument" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Документ сведений по МИВ (Межведомственному Информационному Взаимодействию):
                        - карта-план
                        - описание местоположения
                        - сведения для внесения в реестр ОН
                        - сведения для внесения в реестр Прав
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="legalAct" type="tns:TLegalAct" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Правоустанавливающий документ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="confirmPrivilege" type="tns:TConfirmPrivilege" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Документ, подтверждающий льготу</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:choice>
    </xs:complexType>
    <xs:complexType name="TSomeDocument">
        <xs:annotation>
            <xs:documentation>Набор документов прикладываемых к заявлению</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="tns:TBasicSomeDocument">
                <xs:choice>
                    <xs:element name="otherDocument" type="tns:TOtherDocumentRestr">
                        <xs:annotation>
                            <xs:documentation>Прочий документ</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="paymentDocument" type="tns:TPaymentDocumentRestr">
                        <xs:annotation>
                            <xs:documentation>Платежный документ</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="idDocument" type="tns:TIdDocumentRestr">
                        <xs:annotation>
                            <xs:documentation>Документ, удостоверяющий личность</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="powerOfAttorney" type="tns:TPowerOfAttorneyRestr">
                        <xs:annotation>
                            <xs:documentation>Доверенность</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="mapPlanDocument" type="tns:TMapPlanDocumentRestr">
                        <xs:annotation>
                            <xs:documentation>Тех. план/межевой план/акт обследования/карта-план</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="legalAct" type="tns:TLegalActRestr">
                        <xs:annotation>
                            <xs:documentation>Правоустанавливающий документ</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="confirmPrivilege" type="tns:TConfirmPrivilege">
                        <xs:annotation>
                            <xs:documentation>Документ, подтверждающий льготу</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:choice>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="TSomeRepresentativeDocument">
        <xs:annotation>
            <xs:documentation>Набор документов подтверждающих полномочия</xs:documentation>
        </xs:annotation>
        <xs:choice>
            <xs:element name="document" type="tns:TStrictDocument">
                <xs:annotation>
                    <xs:documentation>Прочий документ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="powerOfAttorney" type="tns:TPowerOfAttorney">
                <xs:annotation>
                    <xs:documentation>Доверенность</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="legalAct" type="tns:TLegalAct">
                <xs:annotation>
                    <xs:documentation>Правоустанавливающий документ</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:choice>
    </xs:complexType>
    <xs:complexType name="TSomeInterdepDocument">
        <xs:annotation>
            <xs:documentation>Данные о документе по Межведу</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="tns:TBasicSomeDocument">
                <xs:choice>
                    <xs:element name="document" type="tns:TDocument">
                        <xs:annotation>
                            <xs:documentation>Сведения о документе</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="interdepDocument" type="tns:TInterdepDocument">
                        <xs:annotation>
                            <xs:documentation>Документ сведений по МИВ (Межведомственному Информационному Взаимодействию):
                                -карта (план) объекта землеустройства
                                -описание местоположения границ соответствующих территорий или зон
                                -сведения для внесения в реестр ОН
                                -сведения для внесения в реестр Прав
                                -сведения для внесения в реестр границ</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="legalAct" type="tns:TLegalActMIV">
                        <xs:annotation>
                            <xs:documentation>Реквизиты решений, правовых актов, иных документов, являющихся основанием для внесения сведений</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:choice>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="TSomeAppliedDocuments">
        <xs:annotation>
            <xs:documentation>Приложенные документы, для документов, поступающих без заявления</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="tns:TBasicSomeDocument">
                <xs:choice>
                    <xs:element name="document" type="tns:TAppliedDocument" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Документ</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="otherDocument" type="tns:TOtherAppliedDocument">
                        <xs:annotation>
                            <xs:documentation>Прочий документ</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="paymentDocument" type="tns:TPaymentDocumentRestr"
                        minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Платежный документ</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="idDocument" type="tns:TIdAppliedDocument" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Документ, удостоверяющий личность</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="powerOfAttorney" type="tns:TPowerOfAttorneyApplied"
                        minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Доверенность</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="mapPlanDocument" type="tns:TMapPlanDocumentApplied"
                        minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Тех. план/межевой план/акт обследования/карта-план</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="interdepDocument" type="tns:TInterdepDocumentApplied"
                        minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Документ сведений по МИВ (Межведомственному Информационному Взаимодействию):
                        - карта-план
                        - описание местоположения
                        - сведения для внесения в реестр ОН
                        - сведения для внесения в реестр Прав
                    </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="legalAct" type="tns:TLegalActApplied" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Правоустанавливающий документ</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="confirmPrivilege" type="tns:TConfirmPrivilegeApplied"
                        minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Документ, подтверждающий льготу</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:choice>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="TDocument">
        <xs:annotation>
            <xs:documentation>Сведения о документе</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element maxOccurs="2" name="documentTypes" type="tns:DocumentTypes" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Информация по кодам документа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="name" type="tns:SDocName" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Наименование документа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="number" type="tns:SDocNumber" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Номер документа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="issueDate" type="xs:date">
                <xs:annotation>
                    <xs:documentation>Дата выдачи</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="attachment" type="tns:TAttachment" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Описание приложенного файла</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="notes" type="commons:TNote">
                <xs:annotation>
                    <xs:documentation>Особые отметки, со спецификацией типа отметки</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="_id" type="xs:token" use="required">
            <xs:annotation>
                <xs:documentation>Идентификатор документа</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="TAppliedDocument">
        <xs:annotation>
            <xs:documentation>Сведения о документе. Приложенные документы, для документов, поступающих без заявления</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="tns:TDocument">
                <xs:sequence>
                    <xs:element maxOccurs="2" name="documentTypes" type="tns:DocumentTypes"
                        minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Информация по кодам документа</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="name" type="tns:SDocName" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Наименование документа</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="number" type="tns:SDocNumber" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Номер документа</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="issueDate" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>Дата выдачи</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="attachment" type="tns:TAttachment" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Описание приложенного файла</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="unbounded" minOccurs="1" name="notes"
                        type="commons:TNote">
                        <xs:annotation>
                            <xs:documentation>Особые отметки, со спецификацией типа отметки</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="DocumentTypes">
        <xs:choice maxOccurs="1">
            <xs:element name="documentTypeCode" type="dDocument:DDocument" minOccurs="1">
                <xs:annotation>
                    <xs:documentation>Мнемоника типа документа согласно справочника "Документы" ЕС УНСИ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="representativeDocTypeCode" type="tns:DBaseRepresentativeDocument">
                <xs:annotation>
                    <xs:documentation>Мнемоника типа документа подтверждающего полномочия представителя справочника ЕС УНСИ "Документы, подтверждающие полномочия представителя"
Заполняется в случае, если документ выступает в качестве подтверждения полномочий представителя.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:choice>
    </xs:complexType>

    <xs:complexType name="AppliedOtherDocumentTypes">
        <xs:complexContent>
            <xs:restriction base="tns:DocumentTypes">
                <xs:choice maxOccurs="1">
                    <xs:element name="documentTypeCode" minOccurs="1">
                        <xs:annotation>
                            <xs:documentation>Мнемоника типа документа согласно справочника "Документы" ЕС УНСИ</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="dDocument:DDocument">
                                <xs:pattern value="008002\d{6}"/>
                                <xs:pattern value="5581\d{8}"/>
                                <xs:pattern value="5582\d{8}"/>
                                <xs:pattern value="558402\d{6}"/>
                                <xs:pattern value="558403\d{6}"/>
                                <xs:pattern value="558404\d{6}"/>
                                <xs:pattern value="558405\d{6}"/>
                                <xs:pattern value="5585\d{8}"/>
                                <xs:pattern value="5586\d{8}"/>
                                <xs:pattern value="5587\d{8}"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="representativeDocTypeCode" type="tns:DRepresentativeDocument">
                        <xs:annotation>
                            <xs:documentation>Мнемоника типа документа подтверждающего полномочия представителя справочника ЕС УНСИ "Документы, подтверждающие полномочия представителя"
                                Заполняется в случае, если документ выступает в качестве подтверждения полномочий представителя.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:choice>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="AppliedPaymentDocumentTypes">
        <xs:complexContent>
            <xs:restriction base="tns:DocumentTypes">
                <xs:choice maxOccurs="1">
                    <xs:element name="documentTypeCode" minOccurs="1">
                        <xs:annotation>
                            <xs:documentation>Мнемоника типа документа согласно справочника "Документы" ЕС УНСИ</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="dDocument:DDocument">
                                <xs:pattern value="55500\d{7}"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="representativeDocTypeCode" type="tns:DRepresentativeDocument">
                        <xs:annotation>
                            <xs:documentation>Мнемоника типа документа подтверждающего полномочия представителя справочника ЕС УНСИ "Документы, подтверждающие полномочия представителя"
                                Заполняется в случае, если документ выступает в качестве подтверждения полномочий представителя.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:choice>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="AppliedIdDocumentTypes">
        <xs:complexContent>
            <xs:restriction base="tns:DocumentTypes">
                <xs:choice maxOccurs="1">
                    <xs:element name="documentTypeCode" minOccurs="1">
                        <xs:annotation>
                            <xs:documentation>Мнемоника типа документа согласно справочника "Документы" ЕС УНСИ</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="dDocument:DDocument">
                                <xs:pattern value="008001\d{6}"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="representativeDocTypeCode" type="tns:DRepresentativeDocument">
                        <xs:annotation>
                            <xs:documentation>Мнемоника типа документа подтверждающего полномочия представителя справочника ЕС УНСИ "Документы, подтверждающие полномочия представителя"
                                Заполняется в случае, если документ выступает в качестве подтверждения полномочий представителя.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:choice>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="AppliedPowerOfAttorneyDocumentTypes">
        <xs:complexContent>
            <xs:restriction base="tns:DocumentTypes">
                <xs:choice maxOccurs="1">
                    <xs:element name="documentTypeCode"
                        type="tns:DPowerOfAttorneyRepresentativeDocument" minOccurs="1"/>
                    <xs:element name="representativeDocTypeCode"
                        type="tns:DPowerOfAttorneyRepresentativeDocument"/>
                </xs:choice>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="AppliedMapPlanDocumentTypes">
        <xs:complexContent>
            <xs:restriction base="tns:DocumentTypes">
                <xs:choice maxOccurs="1">
                    <xs:element name="documentTypeCode" minOccurs="1">
                        <xs:annotation>
                            <xs:documentation>Мнемоника типа документа согласно справочника "Документы" ЕС УНСИ</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="dDocument:DDocument">
                                <xs:pattern value="558203000000"/>
                                <xs:pattern value="558211010000"/>
                                <xs:pattern value="558219000000"/>
                                <xs:pattern value="558254160000"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="representativeDocTypeCode" type="tns:DRepresentativeDocument">
                        <xs:annotation>
                            <xs:documentation>Мнемоника типа документа подтверждающего полномочия представителя справочника ЕС УНСИ "Документы, подтверждающие полномочия представителя"
                                Заполняется в случае, если документ выступает в качестве подтверждения полномочий представителя.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:choice>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="AppliedLegalActDocumentTypes">
        <xs:complexContent>
            <xs:restriction base="tns:DocumentTypes">
                <xs:choice maxOccurs="1">
                    <xs:element name="documentTypeCode" minOccurs="1">
                        <xs:annotation>
                            <xs:documentation>Мнемоника типа документа согласно справочника "Документы" ЕС УНСИ</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="tns:DAppliedLegalActDocument">
                                <xs:pattern value="558401\d{6}"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="representativeDocTypeCode" type="tns:DRepresentativeDocument">
                        <xs:annotation>
                            <xs:documentation>Мнемоника типа документа подтверждающего полномочия представителя справочника ЕС УНСИ "Документы, подтверждающие полномочия представителя"
                                Заполняется в случае, если документ выступает в качестве подтверждения полномочий представителя.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:choice>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="AppliedLegalActDocumentMIVTypes">
        <xs:complexContent>
