            <xs:pattern value="558401\d{6}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="DBaseRepresentativeDocument">
        <xs:restriction base="dDocument:DDocument">
            <xs:pattern value="558301\d{6}"/>
        </xs:restriction>
    </xs:simpleType>

