<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:subj="http://rosreestr.ru/services/v0.1/commonsMortgage/Subjects"
	xmlns:docs="http://rosreestr.ru/services/v0.1/commonsMortgage/Documents"
	xmlns:siml="http://rosreestr.ru/services/v0.1/commonsMortgage/simple-types"
	xmlns:okv="http://rosreestr.ru/services/v0.1/commonsMortgage/directories/okv"
	xmlns:mgc="http://rosreestr.ru/services/v0.1/commonsMortgage/MgCommons"
	xmlns:obj="http://rosreestr.ru/services/v0.1/commonsMortgage/TObject"
	xmlns:dRight="http://rosreestr.ru/services/v0.1/commonsMortgage/directories/right"
	xmlns:dEn="http://rosreestr.ru/services/v0.1/commonsMortgage/directories/encumbrance"
	xmlns:tns="http://rosreestr.ru/services/v0.1/commonsMortgage/electronicMortgage"
	xmlns:com="http://rosreestr.ru/services/v0.1/commonsMortgage/Commons"
	targetNamespace="http://rosreestr.ru/services/v0.1/commonsMortgage/electronicMortgage"
	elementFormDefault="qualified" attributeFormDefault="unqualified" vc:minVersion="1.1">
	<xs:import namespace="http://rosreestr.ru/services/v0.1/commonsMortgage/Subjects"
		schemaLocation="TSubject.xsd"/>
	<xs:import namespace="http://rosreestr.ru/services/v0.1/commonsMortgage/simple-types"
		schemaLocation="CommonSimpleType.xsd"/>
	<xs:import namespace="http://rosreestr.ru/services/v0.1/commonsMortgage/Documents"
		schemaLocation="TDocument.xsd"/>
	<xs:import namespace="http://rosreestr.ru/services/v0.1/commonsMortgage/directories/okv"
		schemaLocation="Dictionary/DOKV.xsd"/>
	<xs:import namespace="http://rosreestr.ru/services/v0.1/commonsMortgage/MgCommons"
		schemaLocation="MgCommons.xsd"/>
	<xs:import namespace="http://rosreestr.ru/services/v0.1/commonsMortgage/TObject"
		schemaLocation="TObject.xsd"/>
	<xs:import namespace="http://rosreestr.ru/services/v0.1/commonsMortgage/directories/encumbrance"
		schemaLocation="Dictionary/DEncumbrance.xsd"/>
	<xs:import namespace="http://rosreestr.ru/services/v0.1/commonsMortgage/directories/right"
		schemaLocation="Dictionary/DRight.xsd"/>
	<xs:import namespace="http://rosreestr.ru/services/v0.1/commonsMortgage/Commons"
		schemaLocation="Commons.xsd"/>

	<xs:element name="electronicMortgage" type="tns:TElectronicMortgage">
		<xs:annotation>
			<xs:documentation>Электронная закладная</xs:documentation>
		</xs:annotation>
		<xs:unique name="docsElectronicMortage">
			<xs:selector
				xpath=".//docs:idDocument | .//docs:powerOfAttorney | .//docs:otherDocument | .//tns:additionalTerms/tns:attachments/tns:appliedDocument/* | tns:obligation/tns:contractsInfo/docs:contractInfo | tns:pledges/tns:pledge/tns:leaseDescription/tns:legalActLeave | tns:pledges/tns:pledge/tns:dduDescription/tns:legalActDdu"/>
			<xs:field xpath="@_id"/>
		</xs:unique>
		<xs:unique name="subjElectronicMortage">
			<xs:selector
				xpath=".//subj:pledger | .//subj:debtor | .//subj:firstOwner | .//subj:representative"/>
			<xs:field xpath="@_id"/>
		</xs:unique>
		<xs:unique name="objElectronicMortage">
			<xs:selector xpath=".//tns:object | tns:pledges/tns:pledge/tns:dduDescription"/>
			<xs:field xpath="@_id"/>
		</xs:unique>
	</xs:element>
	<xs:element name="agreementAmendments" type="tns:TAgreementAmendments">
		<xs:annotation>
			<xs:documentation>Соглашение о внесении изменений в электронную закладную </xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="TElectronicMortgage">
		<xs:sequence>
			<xs:element name="pledgers" type="subj:TPledger">
				<xs:annotation>
					<xs:documentation>1. Залогодатели</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="debtors" type="subj:TDebtor">
				<xs:annotation>
					<xs:documentation>2. Должники</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="firstOwners" type="subj:TFirstOwner">
				<xs:annotation>
					<xs:documentation>3. Первоначальный владелец закладной</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="obligation" type="tns:TObligation" minOccurs="1">
				<xs:annotation>
					<xs:documentation>4. Обязательство, обеспеченное ипотекой</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="interests" type="tns:TIterests" minOccurs="1">
				<xs:annotation>
					<xs:documentation>5 Размер процентов по денежному обязательству, обеспеченному ипотекой</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="debtRepaymentPlan" type="tns:TDebtRepaymentPlan" minOccurs="1">
				<xs:annotation>
					<xs:documentation>6. Срок уплаты суммы обязательства, обеспечиваемого ипотекой (план погашения долга)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="pledges" type="tns:TPledges" minOccurs="1">
				<xs:annotation>
					<xs:documentation>7. Предмет ипотеки</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="additionalTerms" type="tns:TAdditionalTerms" minOccurs="1">
				<xs:annotation>
					<xs:documentation>8. Дополнительные условия</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="depositoryAccounting" type="tns:TDepositoryAccounting" minOccurs="1">
				<xs:annotation>
					<xs:documentation>9. Депозитарный учёта закладной</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="electronicInsteadDocumentary" type="tns:TElectronicInsteadDocumentary"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>10. Информация о выдаче электронной закладной взамен документарной закладной
</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element maxOccurs="1" minOccurs="0" name="registrationInformation"
				type="tns:TRegRight">
				<xs:annotation>
					<xs:documentation>12. Сведения о государственной регистрации ипотеки</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="_id" type="siml:s100">
			<xs:annotation>
				<xs:documentation>Номер закладной. Заполняется в случае выдачи электронной закладной взамен документарной </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="GUID" type="siml:sGUID" use="required">
			<xs:annotation>
				<xs:documentation>Глобальный уникальный идентификатор пакета</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attributeGroup ref="siml:agNeSoftware"/>
	</xs:complexType>
	<xs:complexType name="TAgreementAmendments">
		<xs:annotation>
			<xs:documentation>Соглашение о внесении изменений в электронную закладную </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="pledgers" type="subj:TPledgerAgreement">
				<xs:annotation>
					<xs:documentation>1. Залогодатели</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="debtors" type="subj:TDebtorAgreement">
				<xs:annotation>
					<xs:documentation>2. Должники</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="firstOwners" type="subj:TFirstOwnerAgreement">
				<xs:annotation>
					<xs:documentation>3. Первоначальный владелец закладной</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="obligation" minOccurs="0" type="tns:TObligationModification">
				<xs:annotation>
					<xs:documentation>Внесение изменений в раздел (реквизиты) 4. "Обязательство, обеспеченное ипотекой" </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="interests" type="tns:TIterestsModification">
				<xs:annotation>
					<xs:documentation>Внесение изменений в раздел (реквизиты) 5 "Размер процентов по денежному обязательству"</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="debtRepaymentPlan"
				type="tns:TDebtRepaymentPlanModification">
				<xs:annotation>
					<xs:documentation>Внесение изменений в раздел (реквизиты) 6. "Срок уплаты суммы обязательства"</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element form="qualified" minOccurs="0" name="pledges"
				type="tns:TPledgesModification">
				<xs:annotation>
					<xs:documentation>Внесение изменений в раздел (реквизиты) 7. "Предмет ипотеки"</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="additionalTerms" type="tns:TAdditionalTermsModification">
				<xs:annotation>
					<xs:documentation>Внесение изменений в раздел (реквизиты) 8. "Дополнительные условия"</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="depositoryAccounting"
				type="tns:TDepositoryAccountingModification">
				<xs:annotation>
					<xs:documentation>Внесение изменений в раздел (реквизиты) 9. "Депозитарный учёта закладной"</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="electronicInsteadDocumentary"
				type="tns:TElectronicInsteadDocumentaryModification">
				<xs:annotation>
					<xs:documentation>Внесение изменений в раздел (реквизиты) 10. "Информация о выдаче электронной закладной взамен документарной закладной"
</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="_id" type="siml:s100" use="required">
			<xs:annotation>
				<xs:documentation>Номер закладной</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="GUID" type="siml:sGUID" use="required">
			<xs:annotation>
				<xs:documentation>Глобальный уникальный идентификатор пакета</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attributeGroup ref="siml:agNeSoftware"/>
	</xs:complexType>
	<xs:complexType name="TObligationModification">
		<xs:annotation>
			<xs:documentation>Внесение изменений в раздел (реквизиты) 4. "Обязательство, обеспеченное ипотекой" </xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="replace" type="tns:TObligation">
				<xs:annotation>
					<xs:documentation>Изменить раздел</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="edit" type="tns:TBaseObligation">
				<xs:annotation>
					<xs:documentation>Изменить реквизиты раздела</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="TIterestsModification">
		<xs:annotation>
			<xs:documentation>Внесение изменений в раздел (реквизиты) 5 "Размер процентов по денежному обязательству"</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="replace" type="tns:TIterests">
				<xs:annotation>
					<xs:documentation>Изменить раздел</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="edit" type="tns:TBaseIterests">
				<xs:annotation>
					<xs:documentation>Изменить реквизиты раздела</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="TDebtRepaymentPlanModification">
		<xs:annotation>
			<xs:documentation>Внесение изменений в раздел (реквизиты) 6. "Срок уплаты суммы обязательства"</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="replace" type="tns:TDebtRepaymentPlan">
				<xs:annotation>
					<xs:documentation>Изменить раздел</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="edit" type="tns:TBaseDebtRepaymentPlan">
				<xs:annotation>
					<xs:documentation>Изменить реквизиты раздела</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="TPledgesModification">
		<xs:annotation>
			<xs:documentation>Внесение изменений в раздел (реквизиты) 7. "Предмет ипотеки"</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="replace" type="tns:TPledges">
				<xs:annotation>
					<xs:documentation>Изменить раздел</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="modificationPledges" type="tns:TPledgesEdit">
				<xs:annotation>
					<xs:documentation>Изменение реквизитов предметов ипотеки</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="TPledgesEdit">
		<xs:annotation>
			<xs:documentation>Изменение реквизитов предметов ипотеки</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element maxOccurs="unbounded" name="pledge" type="tns:TModification">
				<xs:annotation>
					<xs:documentation>Изменение реквизитов предмета ипотеки</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TModification">
		<xs:annotation>
			<xs:documentation>Изменение реквизитов предмета ипотеки</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice minOccurs="1">
				<xs:element name="idObject" type="xs:token" minOccurs="1">
					<xs:annotation>
						<xs:documentation>Параметр идентификации - Идентификатор объекта из электронной закладной /tns:electronicMortgage/tns:pledges/tns:pledge/tns:objectDescription/tns:object/@_id или /tns:electronicMortgage/tns:pledges/tns:pledge/tns:leaseDescription/tns:object/@_id или /tns:electronicMortgage/tns:pledges/tns:pledge/tns:dduDescription/@_id либо идентификаторы новых предметов ипотеки из соглашения</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="cadastralNumber" type="com:SCadastralNumber" minOccurs="1">
					<xs:annotation>
						<xs:documentation>Параметр идентификации - Кадастровый номер предмета ипотеки из электронной закладной</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="dduKey" type="tns:TDduKey" minOccurs="1">
					<xs:annotation>
						<xs:documentation>Параметры для идентификации предмета ипотеки по ДДУ
</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="edit" type="tns:TBasePledge" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Изменить реквизиты раздела</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="add" type="tns:TMortgageAdd" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Добавить реквизиты раздела</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="delete" type="tns:TMortgageDelete" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Удалить реквизиты раздела</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TMortgageDelete">
		<xs:annotation>
			<xs:documentation>Тип по удалению сведений о государственной регистрации ипотеки</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="mortgage" type="mgc:TEmpty">
				<xs:annotation>
					<xs:documentation>Сведения о государственной регистрации ипотеки</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="object" type="mgc:TEmpty">
				<xs:annotation>
					<xs:documentation>Удалить предмет ипотеки</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="TMortgageAdd">
		<xs:annotation>
			<xs:documentation>Сведения о добавлении предмета ипотеки либо реквизита премета ипотеки</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="object" type="tns:TPledge">
				<xs:annotation>
					<xs:documentation>Добавить предмет ипотеки</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="mortgage" type="tns:TRegRight">
				<xs:annotation>
					<xs:documentation>Сведения о государственной регистрации ипотеки</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="TDduKey">
		<xs:sequence>
			<xs:element name="numberDdu" type="siml:s100" minOccurs="1">
				<xs:annotation>
					<xs:documentation>Номер ДДУ</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="issueDate" type="xs:date">
				<xs:annotation>
					<xs:documentation>Дата заключения договора</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TAdditionalTermsModification">
		<xs:annotation>
			<xs:documentation>Внесение изменений в раздел (реквизиты) 8. "Дополнительные условия"</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="replace" type="tns:TAdditionalTerms">
				<xs:annotation>
					<xs:documentation>Изменить раздел</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="modification" type="tns:TAdditionalTermsEdit">
				<xs:annotation>
					<xs:documentation>Внести изменения в реквизиты раздела</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="TAdditionalTermsEdit">
		<xs:annotation>
			<xs:documentation>Внести изменения в реквизиты раздела</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="edit" type="tns:TBaseAdditionalTerms" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Изменить реквизиты раздела</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="add" type="tns:TAdditionalTermsAdd" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Добавить реквизиты раздела</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="delete" minOccurs="0" type="tns:TAdditionalTermsDelete">
				<xs:annotation>
					<xs:documentation>Удалить реквизиты раздела</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TAdditionalTermsDelete">
		<xs:sequence>
			<xs:element name="purposeOfCredit" type="mgc:TEmpty" minOccurs="0">
				<xs:annotation>
					<xs:documentation>8.2 Целевое назначение кредита /займа (для обязательств, основанных на кредитном договоре или договоре займа)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="propertyInsurance" type="mgc:TEmpty" minOccurs="0">
				<xs:annotation>
					<xs:documentation>8.4 Сведения о страховании имущества, на которое установлена ипотека</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="insuranceTerms" type="mgc:TEmpty" minOccurs="0">
				<xs:annotation>
					<xs:documentation>8.5 Условия иных видов страхования, предусмотренных условиями договора, обязательства по которому обеспечены ипотекой</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="punishmentTerms" type="mgc:TEmpty" minOccurs="0">
				<xs:annotation>
					<xs:documentation>8.6 Условие о возможности обращения взыскания на заложенное имущество во внесудебном порядке, а также условия реализации заложенного имущества, взыскание на которое обращено во внесудебном порядке</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="pledgeSellingConditions" type="mgc:TEmpty" minOccurs="0">
				<xs:annotation>
					<xs:documentation>8.7 Способы и порядок реализации заложенного имущества при обращении взыскания на предмет ипотеки по решению суда</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="thirdPersonUsage" type="mgc:TEmpty" minOccurs="0">
				<xs:annotation>
					<xs:documentation>8.9 Право и условия предоставления залогодателем заложенного имущества в пользование третьим лицам</xs:documentation>
				</xs:annotation>
			</xs:element>
