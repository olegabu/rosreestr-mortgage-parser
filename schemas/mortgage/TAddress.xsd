        <xs:annotation>
            <xs:documentation>Адрес (описание местоположения) полный</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="address:TAddressCity">
                <xs:sequence>
                    <xs:element name="street" type="address:TAddressElement4Street" minOccurs="0">
                        <xs:annotation>
