                    </xs:element>
                    <xs:element name="other" type="siml:s1000" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Иное местоположение</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="note" type="siml:s1000" minOccurs="0">
                        <xs:annotation>
