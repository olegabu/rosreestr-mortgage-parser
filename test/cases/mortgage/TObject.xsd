	targetNamespace="http://rosreestr.ru/services/v0.1/commonsMortgage/TObject"
	elementFormDefault="qualified"
	xmlns:dIObT="http://rosreestr.ru/services/v0.1/commonsMortgage/directories/interdepobjecttype"
	vc:minVersion="1.1">

	<xs:import namespace="http://rosreestr.ru/services/v0.1/commonsMortgage/Commons"
		schemaLocation="Commons.xsd"/>
	<xs:import namespace="http://rosreestr.ru/services/v0.1/commonsMortgage/Address"
		schemaLocation="TAddress.xsd"/>
	<xs:import namespace="http://rosreestr.ru/services/v0.1/commonsMortgage/directories/roomPurpose"
		schemaLocation="Dictionary/DRoomPurpose.xsd"/>
	<xs:import
		namespace="http://rosreestr.ru/services/v0.1/commonsMortgage/directories/objectPurpose"
		schemaLocation="Dictionary/DObjectPurpose.xsd"/>
	<xs:import
		namespace="http://rosreestr.ru/services/v0.1/commonsMortgage/directories/housingPurpose"
		schemaLocation="Dictionary/DHousingPurpose.xsd"/>
	<xs:import namespace="http://rosreestr.ru/services/v0.1/commonsMortgage/directories/objectType"
		schemaLocation="Dictionary/DObjectType.xsd"/>
	<xs:import namespace="http://rosreestr.ru/services/v0.1/commonsMortgage/directories/unitType"
